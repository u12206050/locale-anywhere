{
    "name": "day4/switch-locale",
    "description": "A Laravel Nova tool to switch the locale",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Day4\\SwitchLocale\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Day4\\SwitchLocale\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
